{
  "name": "boilerplate-nodejs-swagger",
  "version": "1.0.0",
  "description": "node API with Express",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "NODE_ENV=test DB=test jest",
    "test:watch": "NODE_ENV=test DB=test jest --watch",
    "test:cov": "NODE_ENV=test DB=test jest --coverage",
    "start": "node --watch src/index.js",
    "start:test": "NODE_ENV=test DB=test PORT=3001 node --watch src/index.js",
    "prettier": "prettier --write '**/*.js'",
    "pre-commit": "pretty-quick --staged",
    "prepare": "cd .. && husky install server/.husky"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-proxy-middleware": "^2.0.6",
    "passport": "^0.6.0",
    "passport-42": "^1.2.6",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.2",
    "sqlite3": "^5.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "util": "^0.12.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.1",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "sequelize-mock": "^0.10.2"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "verbose": true
  },
  "imports": {
    "#config/*": "./src/config/*.config.js",
    "#controllers/*": "./src/controllers/*.controller.js",
    "#middlewares/*": "./src/middlewares/*.middleware.js",
    "#models/*": "./src/models/*.model.js",
    "#routes/auth": "./src/routes/auth/index.js",
    "#routes/*": "./src/routes/*/*.js",
    "#services/*": "./src/services/*.service.js",
    "#strategies/*": "./src/strategies/*.js",
    "#utils/*": "./src/utils/*.js"
  }
}
